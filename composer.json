{
    "name": "opendialogai/xmpp",
    "description": "An XMPP Laravel package for OpenDialog",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "opendialogai",
        "xmpp"
    ],
    "authors": [
        {
          "name": "Steve McDougall",
          "email": "stevem@greenshootlabs.com"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "opendialogai/core": "dev-feature/response-engine"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.15",
        "matthewbdaly/artisan-standalone": "^0.0.8",
        "opendialogai/dgraph-docker": "^1.1",
        "orchestra/testbench": "^4.0",
        "phpro/grumphp": "^0.16.0",
        "phpunit/phpunit": "^8.3",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "preferred-install": "source",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OpenDialogAi\\Xmpp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "OpenDialogAi\\Xmpp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "OpenDialogAi\\Xmpp\\XmppServiceProvider"
          ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/phpcbf --standard=PSR12 --report=full src/"
    }
}
