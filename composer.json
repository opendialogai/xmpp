{
    "name": "opendialogai/xmpp",
    "description": "An XMPP Laravel package for OpenDialog",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "opendialogai",
        "xmpp"
    ],
    "authors": [
        {
          "name": "Steve McDougall",
          "email": "steveem@greenshootlabs.com"
        }
    ],
    "require": {
        "php": "^7.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": "^4.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "matthewbdaly/artisan-standalone": "^0.0.8",
        "phpro/grumphp": "^0.16.0",
        "squizlabs/php_codesniffer": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "config": {
        "preferred-install": "source",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OpenDialogAi\\XMPP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "OpenDialogAi\\XMPP\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "OpenDialogAi\\XMPP\\XMPPServiceProvider"
          ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "check-style": "./vendor/bin/phpcs src tests",
        "fix-style": "./vendor/bin/phpcbf src tests"
    }
}
